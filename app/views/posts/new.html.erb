<h1>画像投稿フォーム</h1>
<%= form_with model: @post, local:true do |f| %>
  <h4>画像</h4>
  <%= f.attachment_field :post_image %>
  <h4>タイトル</h4>
  <%= f.text_field :post_title %>
  <h4>場所</h4>
  <%= form_for @post, method: :post do |f| %>
  <%= f.select :prefecture, Post.prefectures.keys, {} %>
  <h4>説明</h4>
  <%= f.text_area :post_introduction %>
  <%= f.submit '投稿' %>
  <% end %>
<% end %>